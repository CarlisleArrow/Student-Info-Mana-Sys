<Window x:Class="学生信息管理系统_.NET_Core_.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:学生信息管理系统_.NET_Core_"
        mc:Ignorable="d"
        Title="Dashboard" Height="800" Width="1440" WindowStartupLocation="CenterScreen" WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <WebBrowser x:Name="dashboard" Margin="0,85,0,0"/>
        <Button Content="" HorizontalAlignment="Left" Height="70" VerticalAlignment="Top" Width="97" BorderBrush="{x:Null}" Foreground="{x:Null}" Cursor="No" Click="Button_Click">
            <Button.Background>
                <ImageBrush ImageSource="/back.png" Stretch="Uniform"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
